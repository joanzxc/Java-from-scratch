import javax.swing.*;

public class Text extends JFrame{
 
 private JTextField textofield1;
 private JTextArea textarea1;
 
 public Text(){
  super("Text test");
  setLayout(null);
  textofield1 = new JTextField("");
  textofield1.setBounds(10,10,200,30);
  add(textofield1);
  
  textarea1 = new JTextArea();
  textarea1.setBounds(10,50,250,450);
  add(textarea1);
 }
 
 public static void main(String args[]){
  Text interfaz1 = new Text();
  interfaz1.setBounds(0,0,250,450);
  interfaz1.setVisible(true);
  interfaz1.setResizable(false);
  interfaz1.setLocationRelativeTo(null);
 }
}
